<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.xtremevideos" name="Xtreme Videos" version="1.0.0" provider-name="ad">
	<requires>
		<import addon="xbmc.python" version="2.19.0" />
		<import addon="script.module.nanscrapers" version="0.5.5" />
        <import addon="script.module.python.koding.aio" version="0.0.1"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Xtreme Videos</summary>
		<description lang="en">Xtreme outdoor and sports videos. The developer does not own any of the content within this addon. The developer is in no way, shape, or form affiliated or associated with XBMC or Kodi.</description>
		<platform>all</platform>
		<forum>http://www.noobsandnerds.com/support</forum>
		<website>www.noobsandnerds.com</website>
		<news></news>
		<genre>movies,usa</genre>
		<video_guide></video_guide>
		<video_preview></video_preview>
	</extension>
</addon>

<addon id="repository.xtreme" name="Xtreme" version="0.0.1" provider-name="dp1">
	<extension point="xbmc.addon.repository" name="Xtreme">
		<dir>
			<info compressed="false">https://raw.githubusercontent.com/noobsandnerds/modules4all/master/zips/addons.xml</info>
			<checksum>https://raw.githubusercontent.com/noobsandnerds/modules4all/master/zips/addons.xml.md5</checksum>
			<datadir zip="true">https://raw.githubusercontent.com/noobsandnerds/modules4all/master/zips/</datadir>
		</dir>
		<info compressed="false">https://raw.githubusercontent.com/dpollett/repository.xtreme/master/zips/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/dpollett/repository.xtreme/master/zips/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/dpollett/repository.xtreme/master/zips/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Quick one liner about the repository</summary>
		<description lang="en">Full repository description.</description>
		<genre>documentary,movies,sports,tvshows,uk,usa</genre>
	</extension>
</addon>
</addons>
